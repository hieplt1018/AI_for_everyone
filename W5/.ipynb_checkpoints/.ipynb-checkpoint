    "### VD6: V  th hm ReLU"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 720x360 with 1 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "def relu_func(data):\n",
    "    return (np.abs(data) + data) / 2\n",
    "def relu_derivative(data):\n",
    "    return (data > 0) * 1\n",
    "\n",
    "data = np.arange(-8,8,0.01)\n",
    "data_relu = relu_func(data)\n",
    "data_drelu = relu_derivative(data)\n",
    "\n",
    "# Chnh v tr cc tr  th\n",
    "fig, ax = plt.subplots(figsize=(10,5))\n",
    "ax.spines['left'].set_position('center') #chuyn trc bn tri v trung tm figure\n",
    "ax.spines['right'].set_color('none') #xa trc phi\n",
    "ax.spines['top'].set_color('none') #xa trc trn\n",
    "ax.xaxis.set_ticks_position('bottom') #thit lp trc x l trc di\n",
    "ax.yaxis.set_ticks_position('left') #thit lp trc y l trc tri\n",
    "\n",
    "ax.plot(data, data_relu, color=\"#d35400\", linewidth=3, label=\"relu\")\n",
    "ax.plot(data, data_drelu, color=\"#1abd15\", linewidth=3, label=\"o hm\")\n",
    "ax.legend(loc=\"upper left\", frameon=False) #Hin th ch thch cho  th\n",
    "fig.show()"
   ]
  },
