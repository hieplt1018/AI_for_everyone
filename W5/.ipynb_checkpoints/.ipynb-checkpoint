     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "def sigmoid(x):\n",
    "    return 1/(1+np.e**(-x))\n",
    "\n",
    "x = np.arange(-8, 8, 0.01)\n",
    "y = sigmoid(x)\n",
    "dx = y*(1-y)\n",
    "# Chnh v tr cc tr  th\n",
    "fig, ax = plt.subplots(figsize=(10,5))\n",
    "ax.spines['left'].set_position('center') #chuyn trc bn tri v trung tm figure\n",
    "ax.spines['right'].set_color('none') #xa trc phi\n",
    "ax.spines['top'].set_color('none') #xa trc trn\n",
    "ax.xaxis.set_ticks_position('bottom') #thit lp trc x l trc di\n",
    "ax.yaxis.set_ticks_position('left') #thit lp trc y l trc tri\n",
    "\n",
    "ax.plot(x, y, color=\"#d35400\", linewidth=3, label=\"sigmoid\")\n",
    "ax.plot(x, dx, color=\"#1abd15\", linewidth=3, label=\"o hm\")\n",
    "ax.legend(loc=\"upper left\", frameon=False) #Hin th ch thch cho  th\n",
    "fig.show()"
   ]
  },
